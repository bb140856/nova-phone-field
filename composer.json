{
    "name": "bissolli/nova-phone-field",
    "description": "Nova Phone Number field with a dynamic mask based on the country code inserted by the user.",
    "keywords": [
        "laravel",
        "nova",
        "phone mask",
        "input mask"
    ],
    "homepage": "https://github.com/bissolli/nova-phone-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Bissolli",
            "email": "gustavo.bissolli@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bissolli\\NovaPhoneField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bissolli\\NovaPhoneField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
